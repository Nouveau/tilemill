<form>
<ul class='form fill'>
  <% if (!model.get('id')) { %>
  <li class='text'>
    <div class='description'>Add a new Carto stylesheet. Stylesheets are applied to the project in the order they appear.</div>
  </li>
  <% } %>
  <li>
    <label for='id'><span class='required'>*</span> Filename</label>
    <input type='text' name='id' value='<%= model.get('id') %>' size='40' />
  </li>
  <li class='buttons'>
    <input type='submit' value='Save' />
    <input type='button' value='Cancel' class='cancel' />
  </li>
</ul>
</form>
